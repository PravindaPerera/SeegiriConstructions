        <script type="text/javascript">

            function checkForm(form)
            {
                if (form.new_password.value != "" && form.new_password.value == form.re_new_password.value) {
                    if (form.new_password.value.length < 6) {
                        alert("Error: Password must contain at least six characters!");
                        form.new_password.focus();
                        return false;
                    }

                    re = /[0-9]/;
                    if (!re.test(form.new_password.value)) {
                        alert("Error: password must contain at least one number (0-9)!");
                        form.new_password.focus();
                        return false;
                    }
                    re = /[a-z]/;
                    if (!re.test(form.new_password.value)) {
                        alert("Error: password must contain at least one lowercase letter (a-z)!");
                        form.new_password.focus();
                        return false;
                    }
                    re = /[A-Z]/;
                    if (!re.test(form.new_password.value)) {
                        alert("Error: password must contain at least one uppercase letter (A-Z)!");
                        form.new_password.focus();
                        return false;
                    }
                }
                else {
                    alert("Error: Please check that you've entered and confirmed your password!");
                    form.new_password.focus();
                    return false;
                }

                //alert("You entered a valid password: " + form.new_password.value);
                return true;
            }
            ;

        </script>
        
        <script type="text/javascript">
            function validateForm(form) {
                if (form.FName.value != "") {

                    re = /[0-9]/;
                    if (re.test(form.FName.value)) {
                        alert("Error: Fisrt Name cannot contain any numeric digits");
                        form.FName.focus();
                        return false;
                    }
                }
                if (form.LName.value != "") {

                    re = /[0-9]/;
                    if (re.test(form.LName.value)) {
                        alert("Error: Last Name cannot contain any numeric digits");
                        form.LName.focus();
                        return false;
                    }
                }

                if (form.Contact.value != "") {

                    re = /[a-z]/;
                    if (re.test(form.Contact.value)) {
                        alert("Error: Contact number cannot contain any lowercase letters (a-z)!");
                        form.Contact.focus();
                        return false;
                    }
                    re = /[A-Z]/;
                    if (re.test(form.Contact.value)) {
                        alert("Error: Contact number cannot contain any uppercase letter (A-Z)!");
                        form.Contact.focus();
                        return false;
                    }

                    if (form.Contact.value.length != 10) {
                        alert("Error: Contact number should contain 10 digits!");
                        form.Contact.focus();
                        return false;
                    }

                }
            }
            ;
        </script>
        
        <script language="javascript" type="text/javascript">
            function addNewExpense() {
                $(function()
                {
                    $.ajax({
                        type: 'POST',
                        url: '{{path('newExpense',{'url': url})}}', //the script to call to get data          
                        data: $('form').serialize(), id: $("id").val(), //you can insert url argumnets here to pass to api.php
                        //for example "id=5&parent=6"
                        dataType: 'json', //data format      
                        success: function(response)          //on recieve of reply
                        {
                            var myJSONText = JSON.stringify(response);
                            var obj = JSON.parse(myJSONText);

                            if (obj == "Empty") {
                                var html = '<div class="alert alert-danger" role="alert" style="width :550px"><h4>Expense Type Already Exists !!</h4></div>';
                                $('#expenseOutput').html(html);
                            }
                            else {
                                var html = '<table class="table table-bordered table-hover" style="width: 550px">\n\
    <thead>\n\
<tr>\n\
<th ="">Expense ID</th>\n\
<th ="">Expense Title</th></tr></thead>\n\
<tbody>';
                                for (var i = 0; i < obj.length; i++) {
                                    html += '<tr>';
                                    html += '<td>' + obj[i].expense_id
                                            + '</td>';
                                    html += '<td>' + obj[i].expense_title
                                            + '</td>';
                                    html += '</tr>';
                                }

                                html += '</tbody>\n\
            </table>';

                                html += '<div class="alert alert-success" role="alert" style="width :550px"><h4>Addedd New Expense Type !! Refresh the web page to make chamges effective !!</h4></div>';
                                $('#expenseOutput').html(html);

                            }
                        }
                    });
                });
            }


        </script> 

 

